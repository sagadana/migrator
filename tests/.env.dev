# Mongo DB
MONGO_USER=admin
MONGO_PASS=admin123
MONGO_DB=test-db
MONGO_PRIMARY_HOST=mongo-a
MONGO_REPLICA_1=mongo-b
MONGO_PORT=27017
MONGO_RS=rs0
MONGO_URI="mongodb://${MONGO_USER}:${MONGO_PASS}@${MONGO_PRIMARY_HOST}:${MONGO_PORT},${MONGO_REPLICA_1}:${MONGO_PORT}/${MONGO_DB}?authSource=admin&readPreference=primary"

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASS=""
REDIS_DB=0
REDIS_STATE_DB=1
REDIS_ADDR="${REDIS_HOST}:${REDIS_PORT}"


# Postgres
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=test-postgres-user
POSTGRES_PASS=test-postgres-pass
POSTGRES_DB=test-db
POSTGRES_SSLMODE=disable
POSTGRES_DSN="host=${POSTGRES_HOST} port=${POSTGRES_PORT} user=${POSTGRES_USER} password=${POSTGRES_PASS} dbname=${POSTGRES_DB} sslmode=${POSTGRES_SSLMODE}"

# MySQL
MYSQL_HOST=mysql
MYSQL_PORT=3306
MYSQL_ROOT_PASS=test-mysql-root-pass
MYSQL_USER=test-mysql-user
MYSQL_PASS=test-mysql-pass
MYSQL_DB=test_db
MYSQL_DSN="${MYSQL_USER}:${MYSQL_PASS}@tcp(${MYSQL_HOST}:${MYSQL_PORT})/${MYSQL_DB}?charset=utf8mb4&parseTime=True&loc=Local"
